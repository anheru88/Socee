/* Footer Wrap */

#footerwrap
  background-color #333
  color #777
  padding-top 80px
  padding-bottom 100px

  h3
    font-size 20px
    font-weight 400
    color #fff

  p
    color #fff
    font-size 14px

  #social
    margin 20px auto 0px auto
    display inline-block

  input
    &:focus
      outline 0